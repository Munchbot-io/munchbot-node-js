{
    "name": "munchbot-api",
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@now/node-server"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/app.js"
      }
    ],
    "env": {
        "PAGE_ACCESS_TOKEN": "@page-access-token"
    }
}